FROM 'C:\feps\FEPS\3rd year\Econometrics\PostgreSQL\Financial Analysis\csv_files\us-income-quarterly\us-income-quarterly.csv'
WITH (FORMAT csv, HEADER True, DELIMITER ';');

COPY Balance_Sheet
FROM 'C:\feps\FEPS\3rd year\Econometrics\PostgreSQL\Financial Analysis\csv_files\us-balance-quarterly\us-balance-quarterly.csv'
WITH (FORMAT csv, HEADER True, DELIMITER ';');






